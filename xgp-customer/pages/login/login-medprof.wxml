<!--pages/login/login-medprof.wxml-->
<van-cell-group>
  <van-field
    value="{{ medprofid }}"
    required
    clearable
    label="营养师ID"
    icon="question-o"
    placeholder="请输入营养师ID"
    bind:click-icon="onClickIcon"
    bind:input="onInputMedProfId"
  />

  <van-field
    value="{{ password }}"
    type="password"
    label="密码"
    placeholder="请输入密码"
    required
    border="{{ true }}"
    bind:input="onInputPassword"
  />

  <van-button type="primary" size="large" bind:click="onLogin">登录</van-button>
</van-cell-group>
